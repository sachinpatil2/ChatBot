      $('#capita-so-training-booking-form').submit(function( event ) {
        //when a training form is submitted set cookies for cost, unit and section [may change] to be tracked in live person
        $.cookie('liveperson-training-cost', $('span.class-the-cost').html(), { path: '/' });
        $.cookie('liveperson-training-unit', 'service', { path: '/' });
        $.cookie('liveperson-training-section', 'training', { path: '/' });
      });
    }

    /**
     * Session variables
     */
    //// Brochure downloads
    //if ($.cookie('BrochureDownload')) {
    //  lpTag.vars.push([{ scope: 'session', name: 'BrochureDownload', value: $.cookie('BrochureDownload')}]);
    //};

    /**
     * Function to check the training booking form for errors
     * and return a comma separated list of errors to pass to
     * LivePerson
     */
    function trainingFormErrors() {
      var errorName = '';
      var required = 'field left blank';
      var deduct_count = 1;
      $('#capita-so-training-booking-form .error').each(function(){
        var label = '';
        var message = '';
        var log = true;
        switch ($(this).attr('name')) {
          case 'firstname':
            label = 'First name';
            message = required;
          break;
          case 'surname':
            label = 'Surname';
            message = required;
          break;
          case 'job_title':
            label = 'Job title';
            message = required;
          break;
          case 'email':
            label = 'Email address';
            if ($('input#edit-email').val().length == 0) {
              message = required;
            } else {
              message = 'was invalid';
            }
          break;
          case 'organisation':
            label = 'Organisation';
            message = required;
          break;
          case 'address':
            label = 'Address';
            message = required;
          break;
          case 'town':
            label = 'Town';
            message = required;
          break;  
          case 'postcode':
            label = 'Postcode';
            message = required;
          break;
          case 'purchase_order_no':
            label = 'Purchase order no.';
            message = required;
          break;
          case 'deduct_prepaid':
            label = 'Deduct from prepaid';
            message = required;
            // The deduct field is a radio set and when errors has more than one entry - make sure it's only logged once
            if (deduct_count > 1) {
              log = false;
            }
            deduct_count++;
          break;
          case 'terms[yes]':
            label = 'Terms & conditions';
            message = 'not agreed to';
        }
        if (log) { 
          errorName += ', ' + label + ' ' + message;
        }
      });
      // Remove first two characters from errorName as it will be ', '
      return errorName.substring(2);
    }
  });
  
