function navJavascript() {
  if( $('body.bp-vsmall').length || $('body.bp-small').length ) {
    $('#navigation ul li.depth-1:not(.home)').slideUp(1);
    $('#navigation ul li ul.menu').slideUp('fast');
    //this make the menu hamburger slide up/down the top level list items (ignoring the home link)
    $('button.menu-link').click(function() {
      if( $('#navigation').hasClass('down') ) {
        $('#navigation').addClass('up').removeClass('down');
        $('#navigation ul li.depth-1:not(.home)').slideUp('fast');
      } else {
        $('#navigation').addClass('down').removeClass('up');
        $('#navigation ul li.depth-1:not(.home)').slideDown('fast');
      };
    });
    //add spans
    $('#navigation li.expanded').prepend('<span class="closed">+</span>');
    //span click functions
    $('#navigation li.expanded span').click(function(event) {
      if ( $(this).hasClass('open') ) {
        //is open [-], close the previous nav
        $(this).parent().find('ul.menu').slideUp('fast');//close all sub uls
        $(this).parent().find('ul.menu').removeClass('open');//remove the class of open on the ul
        $(this).parent().find('a.open').removeClass('open');//remove the class of open on the sub a.opens
        $(this).parent().find('span.open').removeClass('open').addClass('closed').text('+');//open class off the child spans, add .closed, change to +
      } else {
        //is closed [+], open the nav, close any others (unless looking at level 3, then close other level 3s but not the level 2)
        //this is essentially replaicated on the a.click function, but is different enough due to the different tags that it's not a function, but this code could be much better
        if ( $(this).next('ul').hasClass('open') ) {
          $(this).next('ul').slideUp('fast');
          $(this).next('ul').removeClass('open');
          $(this).removeClass('open');
        } else {
        //if you open a non-products (3rd level) item close any other navs
          if ( !$(this).parent().parent().hasClass('open') ) {
            $('#navigation a.open').removeClass('open');
            $('#navigation ul.open').slideUp('fast');
            $('#navigation ul.open').removeClass('open');
            $('#navigation span.open').removeClass('open').addClass('closed').text('+');
          } else {
            $('#navigation ul li ul li a.open').removeClass('open');
            $('#navigation ul li ul li ul.open').slideUp('fast');
            $('#navigation ul li ul li ul.open').removeClass('open');
            $('#navigation ul li ul li span.open').removeClass('open').addClass('closed').text('+');
          };
        //open the nav
          $(this).parent().find('> ul.menu').slideDown('fast');//open the next ul
          $(this).parent().find('> ul.menu').addClass('open');//add the class of open to the sub ul
          $(this).next('a').addClass('open');//add the class of open to the a
          $(this).addClass('open').removeClass('closed').text('-');//closed class off the span, add .open, change to -
        };

      };
    });
    //this will do the show hide on sub navs
    $('#navigation li.expanded > a').click(function(event) {
      if ( !$(this).hasClass('open') ) {
        //re enable this if it's decided to keep the double click on root items
        //event.preventDefault();
      };
      if ( $(this).parent().hasClass('expanded') ) {
        //close the other navs
        if ( $(this).next('ul').hasClass('open') ) {
          $(this).next('ul').slideUp('fast');
          $(this).next('ul').removeClass('open');
          $(this).removeClass('open');
        } else {
        //if you open a non-products (3rd level) item close any other navs
          if ( !$(this).parent().parent().hasClass('open') ) {
            $('#navigation a.open').removeClass('open');
            $('#navigation ul.open').slideUp('fast');
            $('#navigation ul.open').removeClass('open');
            $('#navigation span.open').removeClass('open').addClass('closed').text('+');
          } else {
            $('#navigation ul li ul li a.open').removeClass('open');
            $('#navigation ul li ul li ul.open').slideUp('fast');
            $('#navigation ul li ul li ul.open').removeClass('open');
            $('#navigation ul li ul li span.open').removeClass('open').addClass('closed').text('+');
          };
        //open the nav
          $(this).next('ul').slideDown('fast');
          $(this).next('ul').addClass('open');
          $(this).addClass('open');
          $(this).parent().find('> span').addClass('open').removeClass('closed').text('-');
        };
      };
    });
  /*
   * For the 'desktop' sized navigation we use css dropdowns with additional hoverintent
   * this is the same as the pre-responsive, but minus the touch detection
   */
  } else {
    productsNavigation(); //this adds in the view all links
    // If touch screen device: Use a different method
    if (is_touch_device()) {
      // Loop through nav items. If it has sub-nav, add class
      $("#navigation ul li.depth-1").each(function(){
        if ($(this).find('ul').length > 0) {
          $(this).addClass('dropdown');
        }
      })
      // Dropdown nav items click
      $("#navigation ul li.depth-1.dropdown").click(function(){
        // If it's already shown, return true so link is followed
        if ($(this).hasClass('show')) {
          return true;
        }
        // Clear any other shown items
        $("#navigation ul li.depth-1.dropdown.show").toggleClass('show').toggleClass('with-js');
        // Show this item
        $(this).toggleClass('show').toggleClass('with-js');
        // return false to prevent link click
        return false;
      });
    }
    // If not touch, use hoverintent as normal
    else {
      $("#navigation ul li.depth-1").hoverIntent({
        interval: 100, 
        over: drops_show, 
        timeout: 500, 
        out: drops_hide
      });
      $("#navigation ul li.depth-1").addClass('with-js');
    };
  };
};
