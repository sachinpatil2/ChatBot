  //Google analytics
  function recordOutboundLink(link, category, action, timeout) {
    /*
      This file will be removed, but so far it's only being removed from sims
      so only call the event if it's not sims
     */
    if ( $('body').hasClass('site-sims') ) {
      // Do nothing
    } else {
      // timeout variable:
      // If the page needs to redirect to the link provided, set the timeout to true.
      // This is the normal behaviour & should be used for links to other pages.
      // This should be set to false however for links to anything else, for example
      // PDFs / files, or email addresses. Failing to do so will result in the pdf / file / email
      // opening twice.
    
      // Check if timeout has been set - if so, use value provided, if not set to true
      timeout = typeof timeout !== 'undefined' ? timeout : true;
      // Don't set timeout for email links, to prevent two emails from being opened
      // Don't set timeout for PDF links, to prevent PDF from being opened in both a new window &
      // the current window
      if (link.href.indexOf('mailto:') > -1 || link.href.indexOf('.pdf') > -1) {
        timeout = false;
      }
  
      // label is where we are coming from
      // action is where we are going to
      var label = window.location.pathname;
    
      //new style analytics
      // Category = what it is, i.e. banner, brochure, social etc...
      // Action = What was done, i.e. click, download, email etc...
      // Label = url, as generated above
      if (typeof ga !== 'undefined') {
        ga('send', 'event', category, action, label);
      }
      //if the timeout's set
      if (timeout) {
        setTimeout('document.location = "' + link.href + '"', 100);
      }
    }
  }
  
