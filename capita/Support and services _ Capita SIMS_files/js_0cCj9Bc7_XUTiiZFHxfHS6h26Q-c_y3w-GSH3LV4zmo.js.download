  var stringlength = label.length;
  var truncatedlabel = label.substring(0,40);
  // If the length exceeds the truncate limit, add ellipsis
  if (stringlength > 40) {
    return(truncatedlabel + "...");
  }
  else {
    return(truncatedlabel);
  }
}
