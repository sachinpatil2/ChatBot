  if(('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) {
    bool = true;
  }
  return bool;
};


/**
`*   showSupportInfo
     for responsive friendly support tool tip replacement
 */
function showSupportInfo(selector) {
  //function to hide all, or all but .new .esi-slidedown divs
  function hideExisitingESI(hideNew) {
    //slide up the #esi div
    $('.esi-slidedown > div').slideUp( "slow", function() {
      //delete all slidedown divs when the animation's complete
      if(hideNew=="yes") {
        $('.esi-slidedown').remove();
      } else {
        $('.esi-slidedown:not(.new)').remove();
      };
    });
  };
  $('a[href="'+selector+'"]').on('click', function(event) {
    //clear the new class on existing slidedowns
    $('.esi-slidedown').removeClass('new');
    //if there's a slide down already on this one hide it
    if ( $(this).parent().find('.esi-slidedown').length ) { 
      hideExisitingESI("yes");
    //if there isn't a slidedown on this one, check for any others and hide them, then create the one for this
    } else if ( $('.esi-slidedown').length ) {
      //slide up the #esi div
      hideExisitingESI("no");
      //create the containing div, with an extra class of new to stop it getting deleted
      $('<div class="esi-slidedown new"></div>').appendTo($(this).parent());
      //and slide down a clone of #esi
      $(selector).clone().appendTo( $(this).parent().find('.esi-slidedown') ).slideDown();
    } else {
      //create the containing div
      $('<div class="esi-slidedown"></div>').appendTo($(this).parent());
      //and slide down a clone of #esi
      $(selector).clone().appendTo( $(this).parent().find('.esi-slidedown') ).slideDown();  
    };
    event.stopPropagation();
    event.preventDefault();
    $('div.esi-slidedown').on('click', function(event) {
      event.stopPropagation();
    });
  });
  //hide all when clicking anywhere on the document, except the link & div itself (see .stopPropaations in the function above
  $(document).click(function(){
    hideExisitingESI("yes");
  });
};

