/**
`*   cycleFMB
 * cycles through the front page banners
 * it's based on http://www.simonbattersby.com/blog/simple-jquery-image-crossfade/
 * The main additional feature is the initial loop is quicker that the later ones
 */
var $loopLength = 8000;
var $loopLengthSlow = 15000;
var $loopId = 0;
function fadeInText(thisSlide) {
  //IE7 & 8 don't support fade in on transparent bg pngs when there's no bg colour, so just display them
  if ( $('.lt-ie9').length ) {
    $(thisSlide).find('h2').show();
    $(thisSlide).find('div').show();
    $(thisSlide).find('p').show();
  } else {
    $(thisSlide).find('h2').fadeIn(2000,function(){
      $(thisSlide).find('div').fadeIn(1000);
      $(thisSlide).find('p').fadeIn(1500);
    });
  }
};
function cycleFMBInit(){
  nthChildCounter = 1;
  $('#front-main-banner ul li').each(function() {
    $(this).addClass('n-'+nthChildCounter);
    nthChildCounter++;
  });
  //add an active class to the first child
  $('#front-main-banner ul li:first-child').addClass('active');
  //and do the initial fade infade in the initial text
  fadeInText($('#front-main-banner ul li:first-child'));
};
function cycleFMB(){
  var $active = $('#front-main-banner .active');
  var $next = ($active.next().length > 0) ? $active.next() : $('#front-main-banner li:first');
  $next.css('z-index',2);//move the next image up the pile
  $active.fadeOut(1000,function(){//fade out the top image
    $active.css('z-index',1).show().removeClass('active');//reset the z-index and unhide the image
    $next.css('z-index',3).addClass('active');//make the next image the top one
    fadeInText($next);
    $loopId++;
  });
  if ($loopId==1 && $next.hasClass('n-1')) {
    $loopLength = $loopLengthSlow;
  };
  setTimeout( cycleFMB, $loopLength );
};

/**
`*   equalHeight
     based on a jquery selector
     Note: for responsive any calls of equalHeight() need to be called in reEqualHeights() in the jRespond section
 */
function equalHeight(group) {
  $(group).height('auto');//reset the inline style to auto, very useful for jrespond
  var tallest = 0;
  group.each(function() {
    var thisHeight = $(this).height();
    if(thisHeight > tallest) {
      tallest = thisHeight;
    }
  });
  group.height(tallest);
}

